{
	"name": "leak-detection",
	"version": "0.0.0",
	"author": "stevenwdv",
	"repository": "github:stevenwdv/leak-detection",
	"scripts": {
		"install": "npm -w inject run pack && (sed -i '1s;^;// @ts-nocheck\\n;' node_modules/@duckduckgo/autoconsent/lib/tabwrapper.ts || true)",
		"start": "node --require ts-node/register ./src/main.ts",
		"lint": "tsc --noEmit --emitDeclarationOnly false && eslint .",
		"lint-all": "tsc --noEmit --emitDeclarationOnly false && eslint . && npm -w inject run lint && cd ./test/ && tsc --noEmit --emitDeclarationOnly false && eslint .",
		"generate-schema": "npx typescript-json-schema ./tsconfig.json FieldsCollectorOptions --ignoreErrors --noExtraProps --strictNullChecks --required true --out ./src/crawl-config.schema.json",
		"test-server": "http-server ./test/pages/",
		"test": "tap"
	},
	"workspaces": [
		"inject"
	],
	"dependencies": {
		"@puppeteer/replay": "^0.5.1",
		"chalk": "^4.1.2",
		"jsonschema": "^1.4.1",
		"puppeteer": "^15.3.2",
		"ramda": "^0.28.0",
		"tldts": "^5.7.84",
		"tracker-radar-collector": "github:stevenwdv/tracker-radar-collector#32a9d05e",
		"yargs": "^17.5.1"
	},
	"devDependencies": {
		"@types/http-server": "^0.12.1",
		"@types/node": "^17.0.45",
		"@types/ramda": "^0.28.14",
		"@types/tap": "^15.0.7",
		"@types/yargs": "^17.0.10",
		"@typescript-eslint/eslint-plugin": "^5.30.5",
		"@typescript-eslint/parser": "^5.30.5",
		"eslint": "^8.19.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"http-server": "^14.1.1",
		"tap": "^16.3.0",
		"ts-essentials": "^9.2.0",
		"ts-node": "^10.8.2",
		"typescript": "^4.7.4",
		"typescript-json-schema": "^0.54.0"
	}
}
